{
  "extends": "adonis-preset-ts/tsconfig.json",
  "include": [
    "**/*",
    "app/Models/Count.ts"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "baseUrl": ".",
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "outDir": "build",
    "rootDir": "./",
    "sourceMap": true,
    "paths": {
      
      "App/*": [
        "./app/*"
      ],
      "Config/*": [
        "./config/*"
      ],
      "Contracts/*": [
        "./contracts/*"
      ],
      "Database/*": [
        "./database/*"
      ],
      "types": [
        "jest"
      ]
    },
    "types": [
      "@adonisjs/core",
      "@adonisjs/repl",
      "@japa/preset-adonis/build/adonis-typings",
      "@adonisjs/lucid"
    ]
  }
}